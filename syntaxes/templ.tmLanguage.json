{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "templ",
  "scopeName": "source.templ",
  "patterns": [
    {
      "include": "#package-expression"
    },
    {
      "include": "#import-expression"
    },
    {
      "include": "#css-template"
    },
    {
      "include": "#html-template"
    }
  ],
  "repository": {
    "package-expression": {
      "match": "{% (package) (.+) %}",
      "captures": {
        "1": {
          "name": "keyword.control.go"
        },
        "2": {
          "name": "entity.name.package.go"
        }
      }
    },
    "import-expression": {
      "begin": "{% (import) ",
      "beginCaptures": {
        "1": {
          "name": "keyword.control.go"
        }
      },
      "end": "%}",
      "patterns": [
        {
          "include": "source.go"
        }
      ]
    },
    "css-template": {
      "begin": "({% css .+? %})",
      "end": "({% endcss %})",
      "beginCaptures": {
        "1": {
          "patterns": [
            {
              "include": "#css-template-start"
            }
          ]
        }
      },
      "endCaptures": {
        "1": {
          "patterns": [
            {
              "include": "#css-template-end"
            }
          ]
        }
      },
      "patterns": [
        {
          "include": "#css-property"
        }
      ]
    },
    "css-template-start": {
      "begin": "{% (css) ",
      "beginCaptures": {
        "1": {
          "name": "keyword.control.go"
        }
      },
      "end": "%}",
      "patterns": [
        {
          "include": "source.go"
        }
      ]
    },
    "css-template-end": {
      "match": "{% (endcss) %}",
      "captures": {
        "1": {
          "name": "keyword.control.go"
        }
      }
    },
    "css-property": {
      "begin": "^",
      "end": "$",
      "patterns": [
        {
          "include": "#css-property-expression"
        },
        {
          "include": "#css-property-constant"
        }
      ]
    },
